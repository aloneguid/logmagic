{
   "version": "2.1.0",
   "authors": [ "Ivan Gavryliuk" ],
   "title": "LogMagic",
   "copyright": "Copyright (c) 2015-2016 by Ivan Gavryliuk",
   "packOptions": {
      "iconUrl": "https://alonestore.blob.core.windows.net/nuget/logmagic.png",
      "projectUrl": "https://github.com/aloneguid/logmagic",
      "summary": "Logging made easy"
   },
   "buildOptions": {
      "keyFile": "../../logmagic.snk",
      "xmlDoc": true,
      "outputName": "LogMagic"
   },
   "frameworks": {
      "netstandard1.6": {
         "imports": "dnxcore50",
         "dependencies": {
            "NETStandard.Library": "1.6.1",
            "System.Diagnostics.StackTrace": "4.3.0",
            "System.Threading.Thread": "4.3.0",
            "System.Net.NameResolution": "4.3.0"
         },
         "buildOptions": {
            "define": [ "NETSTANDARD" ]
         }
      },
      "net45": {
         "dependencies": {
            "Newtonsoft.Json": "8.0.3"
         },
         "frameworkAssemblies": {
            "System.Net.Http": "4.0.0.0"
         },
         "buildOptions": {
            "define": [ "NETFULL" ]
         }
      }
   },
   "dependencies": {
      "netbox": "1.0.2"
   }
}
